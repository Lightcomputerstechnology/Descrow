// backend/test-payment.js
console.log('Testing payment controller import...');

try {
  const paymentController = require('./controllers/payment.controller');
  console.log('✅ Import successful!');
  console.log('Available exports:', Object.keys(paymentController));
  console.log('initializePayment type:', typeof paymentController.initializePayment);
} catch (error) {
  console.error('❌ Import failed:', error.message);
  console.error('Stack:', error.stack);
}
